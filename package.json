{
  "name": "sizeme-react",
  "version": "0.0.1",
  "description": "SizeMe webstore UI built with React",
  "main": "sizeme.js",
  "author": "Teppo Jalava <teppo.jalava@sizeme.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve",
    "watch": "nodemon --watch webpack.config.ts --watch package.json -e js,jsx,ts,tsx,json ./node_modules/.bin/webpack-dev-server",
    "build": "webpack --mode production --progress",
    "build-local": "yarn build --env cdn=/",
    "build-cdn": "yarn build --env cdn=https://cdn.sizeme.com/store/",
    "stats": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json dist",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w .",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint": "yarn run prettier && yarn run eslint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@reduxjs/toolkit": "^1.9.5",
    "clsx": "^1.2.1",
    "i18next": "^23.1.0",
    "isomorphic-fetch": "^3.0.0",
    "mobile-detect": "^1.3.6",
    "optional-js": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-html5video": "^2.2.0",
    "react-i18next": "^13.0.0",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.1.0",
    "react-tooltip": "^5.21.1",
    "redux": "^4.2.1",
    "redux-actions": "^3.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "shallow-equals": "^1.0.0",
    "universal-cookie": "^3.1.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.64",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.13",
    "@types/react-loadable": "^5.5.7",
    "@types/react-modal": "^3.16.0",
    "@types/redux-actions": "^2.6.2",
    "@types/redux-logger": "^3.0.9",
    "@types/shallow-equals": "^1.0.0",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^9.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "compass-mixins": "^0.12.10",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "nodemon": "^1.12.1",
    "prettier": "^2.8.8",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.1.3",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.9.0"
  },
  "packageManager": "yarn@3.6.0"
}

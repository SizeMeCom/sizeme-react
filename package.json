{
  "name": "sizeme-react",
  "version": "0.0.1",
  "description": "SizeMe webstore UI built with React",
  "main": "sizeme.js",
  "author": "Teppo Jalava <teppo.jalava@sizeme.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack --config webpack.config.js -d --progress",
    "watch": "nodemon --watch webpack.config.js --watch webpack.parts.js --watch package.json ./node_modules/.bin/webpack-dev-server -- --config webpack.config.js -d --env development --progress",
    "build": "webpack -p --env.environment=production --progress",
    "dev": "webpack -p --env.environment=development --progress",
    "build-local": "webpack -p --env.environment=production --progress --env.cdn=dist/",
    "build-cdn": "webpack -p --env.environment=production --progress --env.cdn=https://cdn.sizeme.com/store/",
    "stats": "webpack --env production --profile --json > stats.json"
  },
  "browserslist": [
    "> 5%",
    "not dead",
    "last 2 versions",
    "not ie < 11",
    "not ie_mob < 11"
  ],
  "dependencies": {
    "core-js": "2",
    "font-awesome": "^4.7.0",
    "i18next": "^15.0.8",
    "isomorphic-fetch": "^3.0.0",
    "mobile-detect": "^1.3.6",
    "optional-js": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-html5video": "^2.2.0",
    "react-i18next": "^10.6.1",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "shallow-equals": "^1.0.0",
    "universal-cookie": "^3.1.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^2.0.1",
    "compass-mixins": "^0.12.10",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.12.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",

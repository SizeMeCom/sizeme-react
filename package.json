{
  "name": "sizeme-react",
  "version": "0.0.1",
  "description": "SizeMe webstore UI built with React",
  "main": "sizeme.js",
  "author": "Teppo Jalava <teppo.jalava@sizeme.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve",
    "watch": "nodemon --watch webpack.config.ts --watch package.json ./node_modules/.bin/webpack-dev-server",
    "build": "webpack --mode production --progress",
    "build-local": "yarn build --env cdn=dist/",
    "build-cdn": "yarn build --env cdn=https://cdn.sizeme.com/store/",
    "stats": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json dist"
  },
  "browserslist": [
    "> 5%",
    "not dead",
    "last 2 versions",
    "not ie < 11",
    "not ie_mob < 11"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "core-js": "2",
    "font-awesome": "^4.7.0",
    "i18next": "^15.0.8",
    "isomorphic-fetch": "^3.0.0",
    "mobile-detect": "^1.3.6",
    "optional-js": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-html5video": "^2.2.0",
    "react-i18next": "^10.6.1",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "shallow-equals": "^1.0.0",
    "universal-cookie": "^3.1.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^20.2.5",
    "@types/webpack": "^5.28.1",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "compass-mixins": "^0.12.10",
    "css-loader": "^6.8.1",
    "eslint": "^5.16.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.12.4",
    "git-revision-webpack-plugin": "^5.0.0",
    "nodemon": "^1.12.1",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.1.3",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.9.0"
  },
  "packageManager": "yarn@3.6.0"
}

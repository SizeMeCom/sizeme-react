{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "jsx": true
    },
    "target": "esnext",
    "externalHelpers": true,
    "transform": {
      "react": {
        "runtime": "automatic"
      },
      "optimizer": {
        "simplify": true
      }
    }
  },
  "minify": false,
  "env": {
    "targets": ["> 5%", "not dead", "last 2 versions", "not ie < 11", "not ie_mob < 11"]
  }
}
